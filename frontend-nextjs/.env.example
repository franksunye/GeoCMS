# 复制此文件为 .env.local 并填入实际值

# --------------------------------------------------
# 数据库配置
# --------------------------------------------------
# 数据库提供商: "sqlite" (本地开发) 或 "postgresql" (生产)
# 在 Vercel 上会自动使用 postgresql
DATABASE_PROVIDER=sqlite

# SQLite 数据库路径 (本地开发)
DATABASE_URL=file:./team-calls.db

# Supabase PostgreSQL 连接字符串 (生产环境)
# 从 Supabase Dashboard > Settings > Database 获取
# DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-REF].supabase.co:5432/postgres?pgbouncer=true
# DIRECT_URL=postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-REF].supabase.co:5432/postgres

# --------------------------------------------------
# AI 服务配置
# --------------------------------------------------
# DeepSeek API Key
DEEPSEEK_API_KEY=

# Google Gemini API Key
GEMINI_API_KEY=

# --------------------------------------------------
# 七牛云存储配置
# --------------------------------------------------
QINIU_ACCESS_KEY=
QINIU_SECRET_KEY=
QINIU_BUCKET_NAME_PRIV=fs-go-priv
QINIU_DOWNLOAD_HOST_PRIV=https://priv.fsgo365.cn

# --------------------------------------------------
# WebSocket 配置 (可选)
# --------------------------------------------------
NEXT_PUBLIC_WS_URL=ws://localhost:8000/ws

# --------------------------------------------------
# 日志配置 (可选)
# --------------------------------------------------
LOG_LEVEL=debug


QINIU_ACCESS_KEY=
QINIU_SECRET_KEY=
QINIU_BUCKET_NAME=
QINIU_TOKEN_EXPIRES=3600
QINIU_DOWNLOAD_HOST=
QINIU_BUCKET_NAME_PRIV=
QINIU_DOWNLOAD_HOST_PRIV=